Grammar

    0 $accept: design $end

    1 design: design global_decl
    2       | global_decl

    3 global_decl: function
    4            | function_decl
    5            | const_int_decl
    6            | enum_decl
    7            | static_decl
    8            | class_definition
    9            | struct_definition

   10 $@1: /* empty */

   11 class_definition: CLASS IDENTIFIER opt_inheritance $@1 '{' class_item_list '}'

   12 opt_inheritance: ':' defined_class
   13                | /* empty */

   14 class_item_list: class_item_list class_item
   15                | /* empty */

   16 $@2: /* empty */

   17 class_item: VAR $@2 class_var_def
   18           | VAR alias_tok IDENTIFIER '=' declared_var ';'

   19 $@3: /* empty */

   20 class_item: STRUCT '<' defined_struct '>' $@3 class_struct_def
   21           | STRUCT '<' defined_struct '>' alias_tok IDENTIFIER '=' declared_var ';'

   22 $@4: /* empty */

   23 class_item: CLASS '<' defined_class '>' $@4 method
   24           | opt_void method

   25 class_var_def: var_decl_list ';'
   26              | method

   27 class_struct_def: struct_decl_list ';'
   28                 | method

   29 $@5: /* empty */

   30 method: IDENTIFIER '(' opt_param_list ')' $@5 function_body

   31 $@6: /* empty */

   32 struct_definition: STRUCT IDENTIFIER $@6 '{' struct_item_list '}'

   33 struct_item_list: struct_item_list struct_item
   34                 | /* empty */

   35 struct_item: VAR IDENTIFIER ';'
   36            | STRUCT '<' defined_struct '>' ';'
   37            | STRUCT '<' defined_struct '>' IDENTIFIER ';'

   38 $@7: /* empty */

   39 function: function_proto $@7 function_body

   40 function_body: '{' statement_list '}'
   41              | '{' statement_list label_statement '}'

   42 $@8: /* empty */

   43 function_proto: ret_type IDENTIFIER $@8 opt_funid '(' opt_param_list ')'
   44               | CLASS '<' defined_class '>' IDENTIFIER opt_int '(' opt_param_list ')'

   45 opt_funid: SHAPENUM '(' const_int_val ')'
   46          | OBJECTNUM '(' opt_const_int_val ')'
   47          | opt_const_int_val

   48 opt_const_int_val: const_int_val
   49                  | /* empty */

   50 const_int_val: INT_LITERAL
   51              | IDENTIFIER

   52 opt_int: const_int_val
   53        | /* empty */

   54 statement_block: statement_block_start statement_list '}'
   55                | statement_block_start statement_list label_statement '}'
   56                | label_statement statement

   57 statement_block_start: '{'

   58 statement_list: statement_list statement
   59               | /* empty */

   60 statement: stmt_declaration
   61          | assignment_statement
   62          | if_statement
   63          | while_statement
   64          | array_loop_statement
   65          | function_call_statement
   66          | special_method_call_statement
   67          | return_statement
   68          | statement_block
   69          | converse_statement
   70          | switch_statement
   71          | script_statement
   72          | break_statement
   73          | continue_statement
   74          | goto_statement
   75          | delete_statement
   76          | SAY '(' opt_nonclass_expr_list ')' ';'
   77          | MESSAGE '(' opt_nonclass_expr_list ')' ';'
   78          | answer_statement
   79          | ABORT ';'
   80          | ';'

   81 alias_tok: ALIAS
   82          | '&'

   83 stmt_declaration: VAR var_decl_list ';'
   84                 | VAR alias_tok IDENTIFIER '=' declared_var ';'

   85 $@9: /* empty */

   86 stmt_declaration: STRUCT '<' defined_struct '>' $@9 struct_decl_list ';'
   87                 | STRUCT '<' defined_struct '>' alias_tok IDENTIFIER '=' declared_var ';'

   88 $@10: /* empty */

   89 stmt_declaration: CLASS '<' defined_class '>' $@10 class_decl_list ';'
   90                 | CLASS '<' defined_class '>' alias_tok IDENTIFIER '=' declared_var ';'
   91                 | STRING string_decl_list ';'
   92                 | const_int_decl
   93                 | enum_decl
   94                 | function_decl
   95                 | static_decl

   96 var_decl_list: var_decl_list ',' var_decl
   97              | var_decl

   98 $@11: /* empty */

   99 enum_decl: ENUM IDENTIFIER $@11 opt_enum_type '{' enum_item_list '}' ';'

